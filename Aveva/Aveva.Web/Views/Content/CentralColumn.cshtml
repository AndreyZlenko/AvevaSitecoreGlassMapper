@using Aveva.Models.Content
@using Sitecore.Mvc.Presentation
@model List<BaseViewModel>
@{

}
@foreach (BaseViewModel contentModel in Model)
{
    switch (contentModel.Type)
    {
        case ContentType.BOX:
            @Html.Partial("~/Views/Content/_Box.cshtml", contentModel);
            break;
        case ContentType.LIST:
            @Html.Partial("~/Views/Content/_List.cshtml", contentModel);
            break;
    }
}